		"@turf/rewind": "^6.5.0",
		"@types/d3": "^7.4.0",
		"@types/file-saver": "^2.0.5",
		"@types/shapefile": "^0.6.1",
		"@types/uuid": "^9.0.2",
		"autoprefixer": "^10.4.14",
		"chart.js": "^4.3.0",
		"chartjs-plugin-datalabels": "^2.2.0",
