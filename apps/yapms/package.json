		"svelte-fa": "^3.0.4",
		"svelte-preprocess": "^5.0.4",
		"svelte-typeahead": "4.4.1",
		"tailwindcss": "^3.3.2",
		"theme-change": "^2.5.0",
		"tslib": "^2.5.3",
		"typescript": "^5.1.3",
