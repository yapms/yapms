	"private": true,
	"type": "module",
	"engines": {
		"node": "^18.0.0"
	},
	"scripts": {
		"dev": "dotenv -e ../../.env -- vite dev",
		"build": "dotenv -e ../../.env -- vite build",
		"preview": "vite preview",
		"start": "dotenv -e ../../.env -- node ./build/index.js",
